      mpechoaround $r The psionic storm hurls $r across the realm!
      mpechoat $r {168}The psionic storm hurls you across the realm!
      mpquiet on
      cast 'chill touch' $r
      cast 'curse' $r
      cast 'stability' $r
      mpquiet off
      mpgorand 17000 17725 0 1
      mptransfer $r
      mpforce $r look
      mppurge self
    else
      mpecho The psionic storm rains fireballs!
      mpquiet on
      cast earthquake
      cast earthquake
      cast earthquake
      cast earthquake
      mpquiet off
      mppurge self
    endif
    break
  endif
  mpecho The psionic storm grows in intensity.
~
>fight_prog 100~
if quest (3,1,$i) == 0
  mpmset self quest 3 1 1
  rescale 30999 $n 100
  mpmset self name psionic storm
endif
